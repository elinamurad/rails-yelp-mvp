<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-4">
      <h1 class="my-5 text-center">Restaurants</h1>
      <ul>
        <% @restaurants.each do |restaurant| %>
          <li><%= link_to restaurant.name, restaurant_path(restaurant) %><%= restaurant.phone_number %> <%= restaurant.category %> </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
